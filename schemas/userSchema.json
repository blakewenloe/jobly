{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "title": "The root schema",
    "description": "The root schema comprises the entire JSON document.",
    "default": {},
    "examples": [
        {
            "username": "appleslicer",
            "first_name": "apple",
            "password": "F$@$#FWERF#$%RW",
            "last_name": "slicer",
            "email": "appleslicer2@gmail.com",
            "photo_url": "https://jsonschema.net/assets/svg/logo_blue.svg"
        }
    ],
    "required": [
        "username",
        "first_name",
        "password",
        "last_name",
        "email",
        "photo_url"
    ],
    "properties": {
        "username": {
            "$id": "#/properties/username",
            "type": "string",
            "title": "The username schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "appleslicer"
            ]
        },
        "first_name": {
            "$id": "#/properties/first_name",
            "type": "string",
            "title": "The first_name schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "apple"
            ]
        },
        "password": {
            "$id": "#/properties/password",
            "type": "string",
            "title": "The password schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "F$@$#FWERF#$%RW"
            ]
        },
        "last_name": {
            "$id": "#/properties/last_name",
            "type": "string",
            "title": "The last_name schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "slicer"
            ]
        },
        "email": {
            "$id": "#/properties/email",
            "type": "string",
            "title": "The email schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "appleslicer2@gmail.com"
            ]
        },
        "photo_url": {
            "$id": "#/properties/photo_url",
            "type": "string",
            "title": "The photo_url schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "https://jsonschema.net/assets/svg/logo_blue.svg"
            ]
        }
    },
    "additionalProperties": true
}