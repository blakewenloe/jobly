{
  "$id": "http://example.com/example.json",
  "type": "object",
  "additionalProperties": false,
  "required": [
    "name"
  ],
  "properties": {
    "name": {
      "$id": "/properties/name",
      "type": "string",
      "title": "The Name Schema ",
      "default": "",
      "minLength": 1,
      "maxLength": 55,
      "examples": ["Rithm"]
    },
    "handle": {
      "$id": "/properties/handle",
      "type": "string",
      "title": "The Handle Schema ",
      "default": "",
      "minLength": 1,
      "maxLength": 55,
      "examples": ["rithm"]
    },
    "num_employees": {
      "$id": "/properties/num_employees",
      "type": "number",
      "title": "The num_employees Schema",
      "default": 1,
      "minLength": 1,
      "maxLength": 55,
      "examples": ["rithm"]
    },
    "description": {
      "$id": "/properties/description",
      "type": "string",
      "title": "The description Schema ",
      "default": "",
      "minLength": 1,
      "maxLength": 55,
      "examples": ["Apple company description"]
    },
    "logo_url": {
      "$id": "/properties/logo_url",
      "type": "string",
      "title": "The Logo Schema ",
      "default": "",
      "format": "uri",
      "examples": [
        "https://ih1.redbubble.net/image.396326354.4958/flat,800x800,075,t.u1.jpg"
      ]
    },
    "_token": {
      "$id": "/properties/token",
      "type": "string",
      "title": "The Token Schema ",
      "default": "",
      "examples": [
        "foo123"
      ]
    }
  }
}
